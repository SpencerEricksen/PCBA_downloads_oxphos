import pandas as pd
import numpy as np

df1 = pd.read_csv('ultimate_oxphos_pcassay_dataset.csv', sep="|"  )
df2 = pd.read_csv('./np_score/all_oxphos_npscores.txt')
df2.drop( columns={'smiles'}, inplace=True )
df3 = df2.merge( df1, how='right', on='PUBCHEM_CID' )
df3.to_csv('ultimate_oxphos_pcassay_dataset_npscores.csv', sep="|", index=False )

