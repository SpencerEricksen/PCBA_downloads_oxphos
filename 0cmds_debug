

for f in `ls CID_lists/pcba-aid*.csv`; do echo $f | cut -f2 -d"-" | cut -f1 -d"_" | cut -c4-; done > zlist_cid_lists &

for f in aid_*.csv; do echo $f | cut -f2 -d"_" | cut -f1 -d"."; done > zlist_aid_merged_lists 

for f in `ls fetch_CGIs/pcba-aid*.smi`; do echo $f | cut -f2 -d"-" | cut -f1 -d"_" | cut -c4-; done > zlist_smi_lists &


sort zlist_cid_lists > zlist_cid_lists2
sort zlist_aid_merged_lists > zlist_aid_merged_lists2
sort zlist_smi_lists > zlist_smi_lists2

mv zlist_smi_lists2 zlist_smi_lists
mv zlist_aid_merged_lists2 zlist_aid_merged_lists
mv zlist_cid_lists2 zlist_cid_lists


